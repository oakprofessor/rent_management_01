
  <div id="newbook">
  <div class="col-sm-10 well">
    <div class="header_booked">
      PHÒNG ĐẶT THÀNH CÔNG
    </div>
    <div class="row">
      <div class="col-sm-8">
        <div class="well">
          <div class="labelPost">
            <%= form_for :order do |f| %>
              <%= f.label :Date_Start %>
              <%= f.label :Date_End %>
              <%= f.label :State %>
              <%= f.label :Number_Person %>
              <%= f.label :Number_Bike %>
              <%= f.label :Number_MotorBike %>
            <% end %>
          </div>
          <div class="contentPost">
            <div class="list-product">
              <div class="row">
              <%= @order.date_start %>
              </div>
              <div class="row">
                <%= @order.date_end %>
              </div>
              <div class="row">
                <%= @order.state%>
              </div>
              <div class="row">
                <%= @order.number_person%>
              </div>
              <div class="row">
                <%= @order.number_bike%>
              </div>
              <div class="row">
                <%= @order.number_motobike%>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

